@model SettingListVm

<div class="container">
    <div class="row">
        <div class="col-lg-10 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h2 class="text-secondary my-4">@Model.Key</h2>

                    @if (Model.Value.Contains(".jpg") || Model.Value.Contains(".png"))
                    {
                        <img src="~/img/logo/@Model.Value" alt="" style="width:400px; height:200px;" />

                    }
                    else
                    {
                        <h1>@Model.Value</h1>
                    }
                    <p class="text-secondary my-4">@Model.CreatedDate.ToString("dd.MM.yyyy")</p>
                    <a class="btn btn-dark my-4" asp-action="index">Go Back</a>
                </div>
            </div>
        </div>
    </div>
</div>
